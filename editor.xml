<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_ALL_SOFTWRAPS_SHOWN" value="true" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern useCustomColors="true" pattern="\btodo: \b.*">
      <option name="FOREGROUND" value="407ab1" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bfixme: \b.*">
      <option name="FOREGROUND" value="c33738" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bimportant: \b.*">
      <option name="FOREGROUND" value="c31f1f" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bcheckme: \b.*">
      <option name="FOREGROUND" value="bc498e" />
      <option name="FONT_TYPE" value="3" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
  </component>
</application>